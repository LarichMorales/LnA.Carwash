<Window x:Class="LnA.CarwashBooking.WIndows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LnA.CarwashBooking.WIndows"
        mc:Ignorable="d"
        Title="About" Height="720" Width="908.101">
    <Grid Margin="0,0,0,-555" Height="1244" VerticalAlignment="Top" HorizontalAlignment="Left" Width="900">
        <Grid.RowDefinitions>
            <RowDefinition Height="466*"/>
            <RowDefinition Height="0*"/>
            <RowDefinition Height="515*"/>
            <RowDefinition Height="237*"/>
        </Grid.RowDefinitions>
        <Grid.Background>
            <SolidColorBrush Color="CornflowerBlue"/>
        </Grid.Background>
        <Label Content="" Height="44" VerticalAlignment="Top" RenderTransformOrigin="0.501,0.938" HorizontalAlignment="Left" Width="900">
            <Label.Background>
                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.HotTrackColorKey}}"/>
            </Label.Background>
        </Label>
        <Image HorizontalAlignment="Left" Height="66" VerticalAlignment="Top" Width="114" Source="Logo.png" Margin="53,36,0,0" Stretch="Fill"/>
        <TextBlock HorizontalAlignment="Left" Height="74" Margin="207,44,0,0"  TextWrapping="Wrap" FontSize="24"  VerticalAlignment="Top" Width="522" TextAlignment="Center" Foreground="Gold"><Run Text="L" TextDecorations="Underline"/><Run Text=" "/><Run Foreground="Black" Text="&amp;" TextDecorations="Underline"/><Run Text=" "/><Run Text="A" TextDecorations="Underline"/><LineBreak/><Run Text="Online Car Wash Booking WebApps" TextDecorations="Underline"/></TextBlock>
        <Label Content="Home" Foreground="Gold" FontSize="14" HorizontalAlignment="Left" Height="40" Margin="109,107,0,0" VerticalAlignment="Top" Width="62"/>
        <Label Content="About" Foreground="Gold" FontSize="16" HorizontalAlignment="Left" Height="40" Margin="247,105,0,0" VerticalAlignment="Top" Width="62"/>
        <Label Content="Services" Foreground="Gold" FontSize="16" HorizontalAlignment="Left" Height="40" Margin="386,105,0,0" VerticalAlignment="Top" Width="134" RenderTransformOrigin="0.791,0.7"/>
